{
	"name": "ribachenko.com-svelte",
	"version": "0.0.1",
	"scripts": {
		"get:map": "node --experimental-specifier-resolution=node --loader ts-node/esm src/build/map.ts",
		"get:pdf": "node --experimental-specifier-resolution=node --loader ts-node/esm src/build/pdf.ts",
		"build-config": "rm -f config.js && buildconfig --input config.json --output src/config.ts --env $CONFIG_ENV --module-type typescript",
		"build-config:local": "CONFIG_ENV=local npm run build-config",
		"build-config:prod": "CONFIG_ENV=prod npm run build-config",
		"start": "source ~/.nvm/nvm.sh && nvm use 16 && npm run build-config:local && npm run dev -- --port 4000",
		"dev": "vite dev",
		"build": "npm run build-config:prod && npm run get:map && svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. ./src && eslint . && stylelint '**/*.{css,scss,svelte}'",
		"format": "prettier --write --plugin-search-dir=. ./src"
	},
	"devDependencies": {
		"@poppanator/sveltekit-svg": "^0.3.4",
		"@sveltejs/adapter-node": "^1.0.0-next.78",
		"@sveltejs/kit": "^1.0.0-next.405",
		"@types/lodash-es": "^4.17.6",
		"@types/marked": "^4.0.3",
		"@types/prismjs": "^1.26.0",
		"@types/sharp": "^0.30.4",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"autoprefixer": "^10.4.7",
		"dotenv": "^16.0.1",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"node-fetch": "^3.2.5",
		"postcss": "^8.4.16",
		"postcss-custom-media": "^8.0.2",
		"postcss-html": "^1.5.0",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"puppeteer": "^17.1.2",
		"sharp": "^0.30.6",
		"stylelint": "^14.12.1",
		"stylelint-config-recommended-scss": "^7.0.0",
		"stylelint-config-standard": "^28.0.0",
		"stylelint-scss": "^4.3.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"ts-node": "^10.8.0",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4"
	},
	"type": "module",
	"dependencies": {
		"classnames": "^2.3.1",
		"cookie": "^0.5.0",
		"dayjs": "^1.11.5",
		"front-matter": "^4.0.2",
		"lodash-es": "^4.17.21",
		"marked": "^4.0.16",
		"prismjs": "^1.29.0",
		"sass": "^1.52.1",
		"single-config": "^2.0.1",
		"xss": "^1.0.13"
	}
}
